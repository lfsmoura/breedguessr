{
  "name": "breedguessr",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "db:migrate": "wrangler d1 execute breedguessr-db --local --file=./migrations/0001_create_dogs_table.sql",
    "db:seed": "bun run scripts/seed-db.ts && wrangler d1 execute breedguessr-db --local --file=./migrations/0002_seed_dogs.sql",
    "db:reset": "rm -rf .wrangler/state && bun run db:migrate && bun run db:seed",
    "deploy": "bun run build && wrangler deploy --config dist/server/wrangler.json"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-start": "^1.132.0",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.24",
    "posthog-js": "^1.255.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.15.3",
    "@cloudflare/workers-types": "^4.20251126.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "wrangler": "^4.51.0"
  }
}
